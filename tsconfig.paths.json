{
  "compilerOptions": {
    "paths": {
      "#src": [
        "./src/*"
      ],
      "#apis/*": [
        "./src/apis/*"
      ],
      "#assets/*": [
        "./src/assets/*"
      ],
      "#constants/*": [
        "./src/constants/*"
      ],
      "#context/*": [
        "./src/context/*"
      ],
      "#components/*": [
        "./src/components/*"
      ],
      "#styles/*": [
        "./src/styles/*"
      ],
      "#hooks/*": [
        "./src/hooks/*"
      ],
      "#pages/*": [
        "./src/pages/*"
      ],
      "#utils/*": [
        "./src/utils/*"
      ],
      "#_types/*": [
        "./src/_types/*"
      ],
      "#locales/*": [
        "./src/locales/*"
      ]
    }
  }
}


